{
  "name": "bg3-osiris",
  "displayName": "BG3 Osiris",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "osiris",
        "aliases": ["Osiris", "osiris"],
        "extensions": [".txt"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "osiris",
        "scopeName": "source.osiris",
        "path": "./syntaxes/osiris.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "osiris",
        "path": "./syntaxes/osiris.tmSnippet.json"
      }
    ],
    "themes": [
      {
        "id": "osiris.theme",
        "label": "Osiris",
        "path": "./syntaxes/osiris.tmTheme.json",
        "uiTheme": "vs-dark"
      }
    ],
    "semanticTokenTypes": [
      {
        "id": "osiris_builtin",
        "superType": "function",
        "description": "A built in Osiris procedure, query, or event."
      },
      {
        "id": "proc",
        "superType": "osiris_builtin",
        "description": "An osiris PROC."
      },
      {
        "id": "qry",
        "superType": "osiris_builtin",
        "description": "An osiris QRY."
      },
      {
        "id": "event",
        "superType": "osiris_builtin",
        "description": "An osiris event."
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.103.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.34.0",
    "typescript": "^5.9.2",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
