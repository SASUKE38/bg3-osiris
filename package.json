{
  "name": "bg3-osiris",
  "displayName": "BG3 Osiris",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "contributes": {
    "languages": [
      {
        "id": "osiris",
        "aliases": ["Osiris", "osiris"],
        "extensions": [".txt"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "osiris",
        "scopeName": "source.osiris",
        "path": "./syntaxes/osiris.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "osiris",
        "path": "./syntaxes/osiris.tmSnippet.json"
      }
    ],
    "themes": [
      {
        "id": "osiris.theme",
        "label": "Osiris",
        "path": "./syntaxes/osiris.tmTheme.json"
      }
    ]
  }
}
